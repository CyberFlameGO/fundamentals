<div class="my-account">
  {% include 'my_account__banner' %}
  {% if page.has_form_errors %}
    <div id="notice">
      <p>
        {% if page.form_error_count == 1 %}
          {{ page.form_error_count }}
          {{ 'liquid.views.my_account_billings_page.error.one' | translate }}
          {{ 'liquid.views.my_account_billings_page.msg_error_get.one' | translate }}
        {% else %}
          {{ page.form_error_count }}
          {{ 'liquid.views.my_account_billings_page.error.other' | translate }}
          {{ 'liquid.views.my_account_billings_page.msg_error_get.other' | translate }}
        {% endif %}
      </p>
      <ul>
        {% for error_message in page.form_error_messages %}
          <li>{{ error_message }}</li>
        {% endfor %}
      </ul>
    </div>
  {% endif %}
  <aside>
    {% include 'my_account__menu' %}
  </aside>
  <main id="{{ theme.settings.main_content_id }}">
    {{ page.edit_billings_form }}
  </main>
</div>
