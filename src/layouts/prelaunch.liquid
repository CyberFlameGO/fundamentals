{% comment %}
  The Prelaunch page layout
{% endcomment %}

<!DOCTYPE html>
<html>
  <head>
    {{ required_header_content }}
    {% include 'meta_tags' %}
    {% include 'page_wide_css' %}
    {% include 'theme_wide_css' %}
  </head>
  <body class="{{ page.page_identifier | kebab_case }} {{ theme.preset_name | kebab_case }}">
    <div id="skip-link">
      <a href="#main-content" tabindex="0">Skip to main content</a>
    </div>
    {% section 'page_header' %}
    {{ content_for_layout }}
    {{ required_footer_content }}
    {% include 'site_wide_script' %}
    {% include 'theme_wide_script' %}
    <div class="overlay"></div>
  </body>
</html>
